{
  "name": "@rawify/rootfinder",
  "title": "RootFinder.js",
  "version": "0.0.4",
  "description": "The RAW root finder library for quadratic and cubic polynomials",
  "homepage": "https://raw.org/book/algebra/solving-cubic-equations/",
  "bugs": "https://github.com/rawify/RootFinder.js/issues",
  "keywords": [
    "math",
    "root",
    "cubic",
    "quadratic"
  ],
  "private": false,
  "main": "./dist/rootfinder.js",
  "module": "./dist/rootfinder.mjs",
  "browser": "./dist/rootfinder.min.js",
  "unpkg": "./dist/rootfinder.min.js",
  "types": "./rootfinder.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./rootfinder.d.mts",
        "default": "./dist/rootfinder.mjs"
      },
      "require": {
        "types": "./rootfinder.d.ts",
        "default": "./dist/rootfinder.js"
      },
      "browser": {
        "types": "./rootfinder.d.ts",
        "default": "./dist/rootfinder.min.js"
      },
      "default": {
        "types": "./rootfinder.d.ts",
        "default": "./dist/rootfinder.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "<4.7": {
      "*": [
        "rootfinder.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:rawify/RootFinder.js.git"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/rawify"
  },
  "author": {
    "name": "Robert Eisele",
    "email": "robert@raw.org",
    "url": "https://raw.org/"
  },
  "license": "MIT",
  "engines": {
    "node": "*"
  },
  "scripts": {
    "test": "mocha tests/*.js",
    "build": "crude-build RootFinder"
  },
  "devDependencies": {
    "crude-build": "^0.1.2",
    "mocha": "^11.7.1"
  },
  "dependencies": {
    "complex.js": "^2.4.2"
  }
}